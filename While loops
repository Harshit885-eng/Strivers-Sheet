#include<iostream>
using namespace std;

int main() {
   int n;
   cin>>n;
   int evensum=0;
   int oddsum=0;

   while(n>0)
   {
	   int lastdigit=n%10;
	   n=n/10;
	   if(lastdigit%2==0)
	   {
		   evensum+=lastdigit;
	   }
	   else{
		   oddsum+=lastdigit;
	   }
   }
   cout<<evensum<<" ";
   cout<<oddsum;
	return 0;
}
